{
  "name": "@na3-app/client",
  "version": "1.0.0",
  "private": true,
  "description": "Nova A3 • App — Abra ordens de serviço, imprima etiquetas, gere documentos, online.",
  "keywords": [
    "nova a3 app",
    "nova a3",
    "nova",
    "a3"
  ],
  "homepage": "https://app.novaa3.com.br/",
  "bugs": "https://github.com/santosmarco/na3-app-client/issues",
  "repository": "https://github.com/santosmarco/na3-app-client",
  "license": "MIT",
  "author": "Marco Santos <marco@marcosantos.dev> (https://github.com/santosmarco)",
  "contributors": [
    "Marco Santos <marco@marcosantos.dev> (https://github.com/santosmarco)"
  ],
  "scripts": {
    "build": "craco build",
    "build:assets": "pwa-asset-generator public/icon.svg public/assets -m public/manifest.json -i public/index.html -a '%PUBLIC_URL%' -w",
    "deploy:prod": "yarn build && firebase deploy --only hosting",
    "deploy:test": "yarn build && firebase hosting:channel:deploy test",
    "lint": "eslint src/ --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "prettier:check": "prettier --check src/",
    "prettier:write": "prettier --write src/",
    "scripts:build": "cd scripts && yarn build",
    "scripts:install": "cd scripts && yarn",
    "scripts:vincrement": "cd scripts && yarn meta:version:increment",
    "scripts:vtimestamp": "cd scripts && yarn meta:version:timestamp:update",
    "start": "craco start",
    "test": "craco test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "simple-import-sort",
      "css-modules"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "react-app",
      "react-app/jest",
      "plugin:css-modules/recommended"
    ],
    "rules": {
      "css-modules/no-undef-class": "error",
      "css-modules/no-unused-class": "warn",
      "react/boolean-prop-naming": "warn",
      "react/destructuring-assignment": "warn",
      "react/function-component-definition": "warn",
      "react/jsx-boolean-value": [
        "warn",
        "always"
      ],
      "react/jsx-curly-brace-presence": "warn",
      "react/jsx-fragments": "warn",
      "react/jsx-handler-names": "warn",
      "react/jsx-key": "error",
      "react/jsx-no-constructed-context-values": "error",
      "react/jsx-no-script-url": "error",
      "react/jsx-no-useless-fragment": "error",
      "react/jsx-pascal-case": "warn",
      "react/jsx-props-no-spreading": "error",
      "react/jsx-sort-default-props": "warn",
      "react/jsx-sort-props": "warn",
      "react/no-array-index-key": "error",
      "react/no-danger": "error",
      "react/no-invalid-html-attribute": "error",
      "react/no-multi-comp": "warn",
      "react/no-typos": "warn",
      "react/no-unsafe": "error",
      "react/no-unstable-nested-components": "error",
      "react/prefer-read-only-props": "warn",
      "react/self-closing-comp": "warn",
      "react/sort-comp": "warn",
      "react/sort-prop-types": "warn",
      "react/style-prop-object": "error",
      "react/void-dom-elements-no-children": "error",
      "simple-import-sort/exports": "warn",
      "simple-import-sort/imports": "warn"
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "parserOptions": {
          "project": "./tsconfig.json"
        },
        "plugins": [
          "typescript-sort-keys"
        ],
        "extends": [
          "plugin:@typescript-eslint/recommended",
          "plugin:@typescript-eslint/recommended-requiring-type-checking"
        ],
        "rules": {
          "@typescript-eslint/array-type": [
            "warn",
            {
              "default": "array-simple"
            }
          ],
          "@typescript-eslint/ban-tslint-comment": "warn",
          "@typescript-eslint/consistent-indexed-object-style": "warn",
          "@typescript-eslint/consistent-type-assertions": "error",
          "@typescript-eslint/consistent-type-definitions": [
            "warn",
            "type"
          ],
          "@typescript-eslint/consistent-type-imports": "warn",
          "@typescript-eslint/dot-notation": "error",
          "@typescript-eslint/explicit-function-return-type": "warn",
          "@typescript-eslint/method-signature-style": "error",
          "@typescript-eslint/no-base-to-string": "off",
          "@typescript-eslint/no-confusing-non-null-assertion": "warn",
          "@typescript-eslint/no-confusing-void-expression": "error",
          "@typescript-eslint/no-duplicate-imports": "error",
          "@typescript-eslint/no-dynamic-delete": "error",
          "@typescript-eslint/no-invalid-this": "error",
          "@typescript-eslint/no-invalid-void-type": "error",
          "@typescript-eslint/no-magic-numbers": "off",
          "@typescript-eslint/no-meaningless-void-operator": "error",
          "@typescript-eslint/no-non-null-asserted-nullish-coalescing": "error",
          "@typescript-eslint/no-redeclare": "error",
          "@typescript-eslint/no-require-imports": "error",
          "@typescript-eslint/no-throw-literal": "error",
          "@typescript-eslint/no-unnecessary-boolean-literal-compare": "warn",
          "@typescript-eslint/no-unnecessary-condition": "error",
          "@typescript-eslint/no-unnecessary-type-arguments": "error",
          "@typescript-eslint/no-unused-expressions": "error",
          "@typescript-eslint/no-useless-constructor": "error",
          "@typescript-eslint/non-nullable-type-assertion-style": "error",
          "@typescript-eslint/prefer-includes": "error",
          "@typescript-eslint/prefer-optional-chain": "error",
          "@typescript-eslint/prefer-readonly-parameter-types": "off",
          "@typescript-eslint/prefer-regexp-exec": "error",
          "@typescript-eslint/prefer-string-starts-ends-with": "error",
          "@typescript-eslint/prefer-ts-expect-error": "error",
          "@typescript-eslint/promise-function-async": "error",
          "@typescript-eslint/require-array-sort-compare": "error",
          "@typescript-eslint/return-await": "error",
          "@typescript-eslint/sort-type-union-intersection-members": "warn",
          "@typescript-eslint/switch-exhaustiveness-check": "error",
          "@typescript-eslint/unified-signatures": "warn",
          "typescript-sort-keys/interface": "warn",
          "typescript-sort-keys/string-enum": "warn"
        }
      }
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@react-pdf-viewer/core": "^2.11.0",
    "@react-pdf-viewer/default-layout": "^2.11.0",
    "@react-pdf-viewer/locales": "^1.0.0",
    "@ryuuji3/react-mask-hook": "^1.0.1",
    "@sentry/browser": "^6.16.1",
    "@sentry/integrations": "^6.16.1",
    "@sentry/react": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "animate.css": "^4.1.1",
    "antd": "^4.17.3",
    "axios": "^0.24.0",
    "dayjs": "^1.10.7",
    "detect-browser": "^5.3.0",
    "extenso": "^2.0.1",
    "firebase": "^9.6.1",
    "history": "^4.10.1",
    "jsbarcode": "^3.11.5",
    "jspdf": "^2.4.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.30",
    "pdfjs-dist": "^2.10.377",
    "qrcode": "^1.5.0",
    "react": "^17.0.2",
    "react-css-theme-switcher": "^0.3.0",
    "react-div-100vh": "^0.7.0",
    "react-dom": "^17.0.2",
    "react-firebase-hooks": "^4.0.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.21.2",
    "react-icons": "^4.3.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-use-localstorage": "^3.5.3",
    "redux": "^4.1.2",
    "rooks": "^5.8.0",
    "validator": "^13.7.0",
    "web-vitals": "^2.1.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/extenso": "^2.0.1",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.12",
    "@types/qrcode": "^1.4.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router-dom": "^5.3.2",
    "@types/validator": "^13.7.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "prettier": "^2.5.1",
    "pwa-asset-generator": "^6.0.5",
    "react-app-rewire-alias": "^1.1.5",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "^4.0.3",
    "redux-devtools-extension": "^2.13.9",
    "tslib": "^2.3.1",
    "type-fest": "^2.8.0",
    "typescript": "^4.5.3",
    "utility-types": "^3.10.0",
    "yarn": "^1.22.17"
  },
  "engines": {
    "node": "^14.18.1"
  }
}
