  "scripts": {
    "build": "yarn lint:fix && yarn prettier:write && craco build",
    "build:assets": "pwa-asset-generator public/icon.svg public/assets -m public/manifest.json -i public/index.html -a '%PUBLIC_URL%' -w",
    "deploy:prod": "yarn build && firebase deploy --only hosting",
    "deploy:test": "yarn build && firebase hosting:channel:deploy test",
    "eject": "react-scripts eject",
    "lint": "yarn eslint src/ --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "prettier:check": "yarn prettier --check src/",
    "prettier:write": "yarn prettier --write src/",
    "start": "craco start",
    "test": "craco test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
